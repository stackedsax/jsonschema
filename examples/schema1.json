{
  "type":"object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "properties":{
    "address": {
      "type":"object",
      "properties":{
        "city": {
          "type":"string"
        },
        "streetAddress": {
          "type":"string"
        }
      }
    },
    "age": {
      "type":"number"
    },
    "firstName": {
      "type":"string"
    },
    "lastName": {
      "type":"string"
    },
    "phoneNumber": {
      "type":"array",
      "items":
        {
          "type":"object",
          "properties":{
            "number": {
              "type":"string"
            },
            "type": {
              "type":"string"
            }
          }
        }
    }
  }
}